    <servlet-class>web.GetMotor</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>GetExtremum</servlet-name>
    <servlet-class>web.GetExtremum</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>GetVehicle</servlet-name>
    <servlet-class>web.GetVehicle</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>GetLocation</servlet-name>
    <servlet-class>web.GetLocation</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>GetHistoryMessage</servlet-name>
    <servlet-class>web.GetHistoryMessage</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>GetStatus</servlet-name>
    <servlet-class>web.GetStatus</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>GetMap</servlet-name>
    <servlet-class>web.GetMap</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>GetAllNewestLocation</servlet-name>
    <servlet-class>web.GetAllNewestLocation</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UploadFileServlet</servlet-name>
    <url-pattern>/servlet/UploadFileServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GetAlert</servlet-name>
    <url-pattern>/servlet/GetAlert</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GetMotor</servlet-name>
    <url-pattern>/servlet/GetMotor</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GetExtremum</servlet-name>
    <url-pattern>/servlet/GetExtremum</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GetVehicle</servlet-name>
    <url-pattern>/servlet/GetVehicle</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GetLocation</servlet-name>
    <url-pattern>/servlet/GetLocation</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GetHistoryMessage</servlet-name>
    <url-pattern>/servlet/GetHistoryMessage</url-pattern>
