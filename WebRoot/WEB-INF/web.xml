  <display-name>GuobiaoTest</display-name>
  <servlet>
    <servlet-name>GetAlert</servlet-name>
    <servlet-class>web.GetAlert</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>UploadFileServlet</servlet-name>
    <servlet-class>mobile.UploadFileServlet</servlet-class>
